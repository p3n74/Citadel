{
  "name": "server",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsdown",
    "check-types": "tsc -b",
    "compile": "bun build --compile --minify --sourcemap --bytecode ./src/index.ts --outfile server",
    "dev": "bun run --hot src/index.ts",
    "start": "bun run dist/index.mjs"
  },
  "dependencies": {
    "@Citadel/api": "*",
    "@Citadel/auth": "*",
    "@Citadel/db": "*",
    "@Citadel/env": "*",
    "@hono/trpc-server": "^0.4.0",
    "@trpc/server": "^11.7.2",
    "better-auth": "^1.4.18",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@Citadel/config": "*",
    "@types/bun": "^1.3.4",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "tsdown": "^0.16.5",
    "typescript": "^5"
  }
}
